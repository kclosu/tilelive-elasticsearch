html
  head
    title= title
    link(rel="stylesheet", href="https://unpkg.com/leaflet@1.0.3/dist/leaflet.css",
      integrity="sha512-07I2e+7D8p6he1SIM+1twR5TIrhUQn9+I6yjqD53JQjFiMf8EtC93ty0/5vJTZGF8aAocvHYNEDJajGdNx1IsQ==",
      crossorigin=""
    )
    script(src="https://unpkg.com/leaflet@1.0.3/dist/leaflet.js",
      integrity="sha512-A7vV8IFfih/D732iSSKi20u/ooOfj/AGehOKq0f4vLT1Zr2Y+RX7C+w8A1gaSasGtRUZpF/NZgzSAu4/Gc41Lg==",
      crossorigin=""
    )
    script(src="https://unpkg.com/leaflet.vectorgrid@latest/dist/Leaflet.VectorGrid.bundled.js")
    style(type="text/css").
      body, html, #map {
        height: 100%;
        width: 100%;
        padding: 0;
        margin: 0;
      }
  body
    #map
    script.
      var map = L.map('map').setView([53.3716, 59.0882], 13);
      L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
        attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'
      }).addTo(map);
      L.Path.prototype.options.radius = 6
      var vtlayer = L.vectorGrid.protobuf('/{z}/{x}/{y}.mvt', {
        rendererFactory: L.svg.tile,
        vectorTileLayerStyles: {
          powerline: {
            weight: 3,
            color: '#334369',
            opacity: 1,
            fillColor: '#334369',
            fill: true,
            radius: 6,
            fillOpacity: 0
          }
        }
      }).addTo(map)